<section id="contact">
  <div class="container">
    <h1>Contact</h1>
    <div class="block"></div>
    <div class="row">
      <div class="six columns">
        <label for="exampleRecipientInput">Name</label>
        <input class="u-full-width" type="text" id="name">
      </div>
      <div class="six columns">
        <label for="exampleEmailInput">Email</label>
        <input class="u-full-width" type="email" id="email">
      </div>
    </div>
    <div class="row">
      <label for="exampleMessage">Message</label>
      <textarea class="u-full-width" id="message"></textarea>
      <input class="button-primary" type="submit" value="Submit" onclick="handleSubmit();">
    </div>
  </div>
</section>